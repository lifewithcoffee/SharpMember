@model MemberUpdateVm
@using SharpMember.Core.Data.Models.Community
@using SharpMember.Core.Views.ViewModels

@for (int i = 0; i < Model.ProfileItemViewModels.Count; i++)
{
    <div class="form-group">
        <input asp-for="ProfileItemViewModels[i].Id" type="hidden" />
        <input asp-for="ProfileItemViewModels[i].MemberProfileItemTemplateId" type="hidden" />

        <label>@Model.ProfileItemViewModels[i].ItemName</label>
        <input asp-for="ProfileItemViewModels[i].ItemName" type="hidden" />

        <input asp-for="ProfileItemViewModels[i].ItemValue" class="form-control" />
    </div>
}